<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="p2pchat_prefs" />
        <exclude domain="database" path="p2pchat_database" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="p2pchat_prefs" />
    </device-transfer>
</data-extraction-rules>